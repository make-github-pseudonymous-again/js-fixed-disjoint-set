{
  "name": "@union-find/contiguous",
  "description": "Fixed-size disjoint set data structures for JavaScript",
  "version": "4.0.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://union-find.github.io/contiguous",
  "repository": {
    "type": "git",
    "url": "https://github.com/union-find/contiguous"
  },
  "bugs": {
    "url": "https://github.com/union-find/contiguous/issues"
  },
  "keywords": [
    "data",
    "disjoint",
    "find",
    "fixed",
    "javascript",
    "js",
    "merge",
    "set",
    "structures",
    "union"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "install-hooks": "husky install",
    "lint": "true",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.0",
    "@babel/preset-env": "7.19.0",
    "@babel/register": "7.18.9",
    "@commitlint/cli": "17.1.2",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.2.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.12.0",
    "coveralls": "3.1.1",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "8.0.1",
    "np": "7.6.2",
    "pinst": "3.0.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.9",
    "xo": "0.52.3"
  },
  "ava": {
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  }
}
